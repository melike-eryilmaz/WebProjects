@model List<ECommerce.Areas.Admin.Models.Brands>

<div class="brands_products">
    <!--brands_products-->
    <h2>Brands</h2>
    <div class="brands-name">
        <ul class="nav nav-pills nav-stacked">
            @foreach (var item in Model)
            {
                <li><a href="#"><span class="pull-right">(@item.Products.Count)</span>@item.Name</a></li>
            }
        </ul>
    </div>
</div><!--/brands_products-->
